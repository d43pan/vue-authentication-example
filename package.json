    "html-webpack-plugin": "^2.30.1",
    "jest": "^24.8.0",
    "nightwatch": "^1.1.12",
    "node-notifier": "^5.1.2",
    "optimize-css-assets-webpack-plugin": "^5.0.1",
    "ora": "^1.2.0",
    "portfinder": "^1.0.13",
